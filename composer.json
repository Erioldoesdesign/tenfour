{
    "name": "ushahidi/RollCall",
    "description": "RollCall",
    "keywords": ["ushahidi", "RollCall"],
    "license": "AGPL3",
    "require": {
        "php": ">=5.5.9",
        "dingo/api": "1.0.*@dev",
        "laravel/framework": "5.3.*",
        "lucadegasperi/oauth2-server-laravel": "^5.0",
        "league/oauth2-server": "^4.1",
        "barryvdh/laravel-cors": "^0.8.2",
        "doctrine/dbal": "^2.5",
        "simplesoftwareio/simple-sms": "^3.0",
        "aws/aws-php-sns-message-validator": "^1.1",
        "zendframework/zend-mail": "^2.7",
        "aws/aws-sdk-php": "^3.20",
        "league/csv": "^8.0",
        "dingo/blueprint": "dev-master",
        "intervention/image": "^2.3",
        "giggsey/libphonenumber-for-php": "^8.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "markdunphy/ses-sns-types": "^0.1.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^2.2",
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~4.0",
        "codeception/codeception": "2.2.*",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "RollCall\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled --no-interaction",
            "php artisan optimize --no-interaction"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled --no-interaction"
        ],
        "post-update-cmd": [
            "php artisan optimize --no-interaction"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "test": [
            "codecept run"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
